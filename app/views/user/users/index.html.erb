<p id="notice"><%= notice %></p>

<h1>Listing User Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Mail</th>
      <th>Login</th>
      <th>Id supplier</th>
      <th>Id equipment</th>
      <th>Id contract type</th>
      <th>Id location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_users.each do |user_user| %>
      <tr>
        <td><%= user_user.Name %></td>
        <td><%= user_user.Mail %></td>
        <td><%= user_user.Login %></td>
        <td><%= user_user.id_Supplier %></td>
        <td><%= user_user.id_Equipment %></td>
        <td><%= user_user.id_Contract_type %></td>
        <td><%= user_user.id_Location %></td>
        <td><%= link_to 'Show', user_user %></td>
        <td><%= link_to 'Edit', edit_user_user_path(user_user) %></td>
        <td><%= link_to 'Destroy', user_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_user_path %>
