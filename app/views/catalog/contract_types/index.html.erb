<p id="notice"><%= notice %></p>

<h1>Listing Catalog Contract Types</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Supplier</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @catalog_contract_types.each do |catalog_contract_type| %>
      <tr>
        <td><%= catalog_contract_type.Type %></td>
        <td><%= catalog_contract_type.Supplier %></td>
        <td><%= link_to 'Show', catalog_contract_type %></td>
        <td><%= link_to 'Edit', edit_catalog_contract_type_path(catalog_contract_type) %></td>
        <td><%= link_to 'Destroy', catalog_contract_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contract type', new_catalog_contract_type_path %>
