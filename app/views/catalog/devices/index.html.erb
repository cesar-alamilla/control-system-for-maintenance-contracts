<p id="notice"><%= notice %></p>

<h1>Listing Catalog Devices</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Stock number</th>
      <th>Location</th>
      <th>Provider who maintains</th>
      <th>User</th>
      <th>Supplier</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @catalog_devices.each do |catalog_device| %>
      <tr>
        <td><%= catalog_device.Name %></td>
        <td><%= catalog_device.Stock_number %></td>
        <td><%= catalog_device.Location %></td>
        <td><%= catalog_device.Provider_who_maintains %></td>
        <td><%= catalog_device.User_id %></td>
        <td><%= catalog_device.Supplier_id %></td>
        <td><%= catalog_device.Location_id %></td>
        <td><%= link_to 'Show', catalog_device %></td>
        <td><%= link_to 'Edit', edit_catalog_device_path(catalog_device) %></td>
        <td><%= link_to 'Destroy', catalog_device, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Device', new_catalog_device_path %>
