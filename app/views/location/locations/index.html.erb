<p id="notice"><%= notice %></p>

<h1>Listing Location Locations</h1>

<table>
  <thead>
    <tr>
      <th>Laboratory department</th>
      <th>Manager</th>
      <th>Mail</th>
      <th>User</th>
      <th>Device</th>
      <th>Contract type</th>
      <th>Supplier</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @location_locations.each do |location_location| %>
      <tr>
        <td><%= location_location.Laboratory_Department %></td>
        <td><%= location_location.Manager %></td>
        <td><%= location_location.Mail %></td>
        <td><%= location_location.User_id %></td>
        <td><%= location_location.Device_id %></td>
        <td><%= location_location.Contract_type_id %></td>
        <td><%= location_location.Supplier_id %></td>
        <td><%= link_to 'Show', location_location %></td>
        <td><%= link_to 'Edit', edit_location_location_path(location_location) %></td>
        <td><%= link_to 'Destroy', location_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Location', new_location_location_path %>
